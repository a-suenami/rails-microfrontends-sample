<p id="notice"><%= notice %></p>

<h1>Product Service Products</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_service_products.each do |product_service_product| %>
      <tr>
        <td><%= product_service_product.name %></td>
        <td><%= product_service_product.price %></td>
        <td><%= link_to 'Show', product_service_product %></td>
        <td><%= link_to 'Edit', edit_product_service_product_path(product_service_product) %></td>
        <td><%= link_to 'Destroy', product_service_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Service Product', new_product_service_product_path %>
